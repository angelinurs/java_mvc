<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="shop">

	<select id="list" parameterType="String" resultType="shoppingBasket.vo.ProductVO">
		SELECT * FROM `employees`.`shop_t`
		WHERE category = #{no}
	</select>

</mapper>